{
  "name": "karizma-connection-js",
  "version": "0.1.0",
  "description": "KarizmaConnection for both server (Node) and client (Node or browser).",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "npm run build",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "signalr",
    "server",
    "client"
  ],
  "author": "KarizmaGameStudio",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  }
}
